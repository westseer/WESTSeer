<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="WESTSeerFrame">
		<title>Worldwide Emerging Scientific Topic Seer </title>
		<icon>westseer-logo.ico</icon>
		<size>800,600</size>
		<id_arg>0</id_arg>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>1</cols>
			<rows>3</rows>
			<growablecols>0</growablecols>
			<growablerows>1,2</growablerows>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>3</cols>
					<rows>3</rows>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Scope:</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxChoice" name="ID_CHOICE1" variable="ChoiceScope" member="yes">
							<style>wxVSCROLL|wxHSCROLL</style>
							<handler function="OnChoiceScopeSelect" entry="EVT_CHOICE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="ButtonRefresh" member="yes">
							<label>Refresh</label>
							<handler function="OnButtonRefreshClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>Highlight 1:</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxChoice" name="ID_CHOICE2" variable="ChoiceHL1" member="yes">
							<style>wxVSCROLL|wxHSCROLL</style>
							<handler function="OnChoiceHL1Select" entry="EVT_CHOICE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxRadioBox" name="ID_RADIOBOX1" variable="RadioBox1" member="yes">
							<label></label>
							<content>
								<item>R</item>
								<item>G</item>
								<item>B</item>
							</content>
							<default>1</default>
							<font>
								<size>8</size>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>@System</face>
							</font>
							<style>wxRA_VERTICAL</style>
							<handler function="OnRadioBox1Select" entry="EVT_RADIOBOX" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
							<label>Highlight 2:</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxChoice" name="ID_CHOICE3" variable="ChoiceHL2" member="yes">
							<style>wxVSCROLL|wxHSCROLL</style>
							<handler function="OnChoiceHL2Select" entry="EVT_CHOICE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxRadioBox" name="ID_RADIOBOX2" variable="RadioBox2" member="yes">
							<label></label>
							<content>
								<item>R</item>
								<item>G</item>
								<item>B</item>
							</content>
							<default>2</default>
							<font>
								<size>8</size>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>@System</face>
							</font>
							<style>wxRA_VERTICAL</style>
							<handler function="OnRadioBox2Select" entry="EVT_RADIOBOX" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxListCtrl" name="ID_LISTCTRL1" variable="ListCtrlPublications" member="yes">
					<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
					<handler function="OnListCtrlPublicationsItemSelect" entry="EVT_LIST_ITEM_SELECTED" />
					<handler function="OnListCtrlPublicationsItemRClick" entry="EVT_LIST_ITEM_RIGHT_CLICK" />
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxNotebook" name="ID_NOTEBOOK1" variable="NotebookInfo" member="yes">
					<object class="notebookpage">
						<object class="wxListCtrl" name="ID_LISTCTRL2" variable="ListCtrlReferences" member="yes">
							<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
						</object>
						<label>References</label>
					</object>
					<object class="notebookpage">
						<object class="wxListCtrl" name="ID_LISTCTRL3" variable="ListCtrlCitations" member="yes">
							<pos>225,140</pos>
							<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
							<handler function="OnListCtrlCitationsItemRClick" entry="EVT_LIST_ITEM_RIGHT_CLICK" />
						</object>
						<label>Citations</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextCtrlAbstract" member="yes">
							<value>Text</value>
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Abstract</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="TextCtrlTopic" member="yes">
							<value>Text</value>
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Topic</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="TextCtrlPrediction" member="yes">
							<value>Text</value>
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Prediction Summary</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL4" variable="TextCtrlVerification" member="yes">
							<value>Text</value>
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Verification Summary</label>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENUITEM10" variable="MenuItem9" member="yes">
					<label>&amp;Add Scope</label>
					<handler function="OnMenuAddScopeSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM3" variable="MenuItemOptions" member="yes">
					<label>&amp;Options</label>
					<handler function="OnMenuItemOptionsSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM4" variable="MenuItem6" member="yes">
					<label>&amp;Export Web-of-Science Data</label>
					<handler function="OnExportWoSSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM5" variable="MenuItem7" member="yes">
					<label>&amp;Save Results</label>
					<handler function="OnSaveResultsSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>&amp;Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>&amp;Mode</label>
				<object class="wxMenuItem" name="ID_MENUITEM2" variable="MenuItem4" member="yes">
					<label>&amp;Test</label>
					<handler function="OnTextModeSelected" entry="EVT_MENU" />
					<radio>1</radio>
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuItem3" member="yes">
					<label>&amp;Explore</label>
					<handler function="OnExploreModeSelected" entry="EVT_MENU" />
					<radio>1</radio>
				</object>
			</object>
			<object class="wxMenu" variable="Menu4" member="yes">
				<label>&amp;View</label>
				<object class="wxMenuItem" name="ID_MENUITEM9" variable="MenuItem8" member="yes">
					<label>&amp;Schedules</label>
					<handler function="OnMenuViewSchedulesSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM8" variable="MenuItem5" member="yes">
					<label>&amp;Downloads</label>
					<handler function="OnMenuViewDownloadsSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM6" variable="MenuItemSQL" member="yes">
					<label>&amp;SQL</label>
					<handler function="OnMenuItemSQLSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM7" variable="MenuItemLog" member="yes">
					<label>&amp;Log</label>
					<handler function="OnMenuItemLogSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>&amp;Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>&amp;About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="Timer1" member="yes">
			<interval>1000</interval>
			<handler function="OnTimer1Trigger" entry="EVT_TIMER" />
		</object>
	</object>
</wxsmith>
