<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="FinishedScopeDialog">
		<title>Finished Scope Inspector</title>
		<size>800,600</size>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxCAPTION|wxCLOSE_BOX|wxMAXIMIZE_BOX|wxMINIMIZE_BOX</style>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>1</cols>
			<rows>3</rows>
			<growablecols>0</growablecols>
			<growablerows>1,2</growablerows>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>2</cols>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Scope:</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxChoice" name="ID_CHOICE1" variable="ChoiceScope" member="yes">
							<handler function="OnChoiceScopeSelect" entry="EVT_CHOICE" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>Mode:</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxRadioBox" name="ID_RADIOBOX1" variable="RadioBoxMode" member="yes">
							<label></label>
							<content>
								<item>Test</item>
								<item>Explore</item>
							</content>
							<default>-1</default>
							<handler function="OnRadioBoxModeSelect" entry="EVT_RADIOBOX" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxListCtrl" name="ID_LISTCTRL1" variable="ListCtrlPublications" member="yes">
					<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
					<handler function="OnListCtrlPublicationsItemSelect" entry="EVT_LIST_ITEM_SELECTED" />
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxNotebook" name="ID_NOTEBOOK1" variable="NotebookInfo" member="yes">
					<object class="notebookpage">
						<object class="wxListCtrl" name="ID_LISTCTRL2" variable="ListCtrlReferences" member="yes">
							<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
						</object>
						<label>References</label>
					</object>
					<object class="notebookpage">
						<object class="wxListCtrl" name="ID_LISTCTRL3" variable="ListCtrlCitations" member="yes">
							<style>wxLC_REPORT|wxVSCROLL|wxHSCROLL</style>
						</object>
						<label>Citations</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextCtrlAbstract" member="yes">
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Abstract</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="TextCtrlTopic" member="yes">
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Topic</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="TextCtrlPrediction" member="yes">
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Prediction Summary</label>
					</object>
					<object class="notebookpage">
						<object class="wxTextCtrl" name="ID_TEXTCTRL4" variable="TextCtrlVerification" member="yes">
							<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
						</object>
						<label>Verification Summary</label>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
